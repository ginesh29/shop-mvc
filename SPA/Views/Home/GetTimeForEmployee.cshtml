@model IEnumerable<SPA.Models.TimeSlots>
@{
    Layout = null;
}

@foreach (var item in Model)
{
    DateTime time = DateTime.Today.Add(DateTime.Parse(item.T1.ToString()).TimeOfDay);
    string displayTime = time.ToString("HH:mm");
    <a class="btn btn-time spa-label">@displayTime</a>
}
@if (Model.Count() == 0)
{
    <div class="Time_off margin-top10">
        <img src="~/images/timer-off.png" />
    </div>
}
