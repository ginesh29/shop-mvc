@model SPA.CCTSP_CategoryDetails
@{
    ViewBag.Title = "AddTimeSlot";
}

@using (Html.BeginForm("AddTimeSlot", "Employee", FormMethod.Post, new { @class = "form-inline" }))
{
    <div class="col-lg-16 col-lg-offset-4">
        <h1 class="title14-border">Add Time Slot</h1>
        <div class="form-group">
            @Html.Label("Time Slot", htmlAttributes: new { @class = "form-label spa-label" })
            @Html.TextBoxFor(c => c.CatgDesc, new { @class = "spa-text-box form-control" })
        </div>
        <div class="form-group">
            <input type="submit" class="btn btn-primary" value="Add">
        </div>
    </div>
}

